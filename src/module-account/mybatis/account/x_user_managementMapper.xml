<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yemh.xqsrd.account.mapper.IXUserMapper">
    <!-- 结果集 -->
    <resultMap id="BaseResultMap" type="xuser" >
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="username" property="username" jdbcType="VARCHAR" />
        <result column="passnord" property="password" jdbcType="VARCHAR" />
        <result column="login_name" property="loginName" jdbcType="VARCHAR" />
<!--         <result column="user_sex" property="userSex" javaType="com.neo.enums.UserSexEnum"/> -->
    </resultMap>
	
	<!-- 用户查询List -->
    <select id="getUserList" parameterType="Map" resultMap="BaseResultMap">
        select *
        from x_user
    </select>
</mapper>